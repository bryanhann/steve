#!/bin/bash
[ -z "${STEVE_FIRST}" ] && {
    export STEVE_FIRST=1
    export PATH=${PATH}:${STEVE}/bin
    export PYTHONPATH=${STEVE}/lib
}


. steve.sys:globals
  steve.sys.clone-manifest
. steve.sys:add-packages
. steve.sys:install-bryan

. bh.bch:activate
. bh.monty:activate
  bh.vim.configure

. steve.path:add ${HOME}/.local/bin
. steve.path:clean

